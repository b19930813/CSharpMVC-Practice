@*Model的資料為RouteTest TempProduct的資料型態*@
@*@Model List<main.Models.RouteTest.TempProduct>*@


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";



    //宣告
}

<h2>商品列表</h2>

<table class="table">
    <thead><tr><td>編號</td><<td>名稱</td><td>售價</td></tr></thead>
    <tbody>
        @foreach (var pd in this.Model)
        {
            <tr>
                <td>@pd.ID</td>
                <td>@pd.Name</td>
                <td>@pd.Price</td>
            </tr>
        }
    </tbody>
</table>